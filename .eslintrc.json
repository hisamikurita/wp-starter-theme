{
	"root": true,
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "prettier"],
	"rules": {
		"no-console": "error"
	},
	"ignorePatterns": ["/node_modules/", "/dist/", "/plugins/"]
}
